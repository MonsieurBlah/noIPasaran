extend layout

block specific-css
	!= css('country')

block content
	.row
		.span12
			table.table.table-bordered.table-striped.table-hover.table-condensed
				thead
					th ISP name
					th 
				tbody
					each item in data.answer
						tr(class=item.valid=="ok"?'success':'error')
							td= item.name
							td
								if item.valid == "fail"
									p Not valid
								else if item.valid == "ok"
									p Valid
								else
									p Timeout
